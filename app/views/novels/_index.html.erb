<section class="">
  <h5>投稿一覧</h5>
  <% workspace.novels.each do |novel| %>
    <table class="table">
      <tr>
        <th>name</th>
        <td><%= link_to novel.chapter, workspace_novel_path(workspace, novel.id) %></td>
        <th>投稿日</th>
        <td><%= novel.created_at.strftime('%Y/%m/%d %H:%M') %></td>
        <th>更新日</th>
        <td><%= novel.updated_at.strftime('%Y/%m/%d %H:%M') %></td>
      </tr>
    </table>
  <% end %>
</section>
